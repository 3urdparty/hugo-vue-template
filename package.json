{
  "name": "hugo-vue-template",
  "version": "1.0.0",
  "priavte": true,
  "description": "A starter template for Hugo projects with Vue.js and Vue Single File Components, complete with webpack. (it is from jekyll-vue-template)",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "browser-sync start --config browser-sync.js",
    "start": "npm run serve",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "repository": {
    "private": true
  },
  "keywords": [
    "hugo",
    "vue",
    "ssg"
  ],
  "author": "daihyun99",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "gsap": "^2.1.3",
    "vue": "^2.6.10",
    "vue-grid-layout": "^2.3.4",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "sass-loader": "^14.1.1",
    "vue-loader": "^17.4.2",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  }
}
